<div class="cont_margin" >
	<h2>NUESTROS COLABORADORES</h2>
	<%@users.each do |u|%>
		<div style="margin-top: 2%; margin-bottom: 1%; position:relative; left: 5%;">
			<h3><%= u.first_name %> <%= u.last_name %></h3>
			<h6><%= u.cargo %> @ <%= u.inst %></h6>
			<p> <%= u.bio %></p>
			<a href="http://<%=u.sitio %>"><%=u.sitio %></a>
		</div>
	<% end %>
</div>


