<h1>Listing tiras</h1>

<table>
  <thead>
    <tr>
      <th>Autor</th>
      <th>Título</th>
      <th>Texto 1</th>
      <th>Texto 2</th>
      <th>Texto 3</th>
      <th>Texto 4</th>
      <th>Texto 5</th>
      <th>Texto 6</th>
      <th>Día</th>
      <th>Mes</th>
      <th>Año</th>
      <th>Aprobado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tiras.each do |tira| %>
      <tr>
        <td><%= tira.user %></td>
        <td><%= tira.titulo %></td>
        <td><%= tira.text1 %></td>
        <td><%= tira.text2 %></td>
        <td><%= tira.text3 %></td>
        <td><%= tira.text4 %></td>
        <td><%= tira.text5 %></td>
        <td><%= tira.text6 %></td>
        <td><%= tira.dia %></td>
        <td><%= tira.mes %></td>
        <td><%= tira.ano %></td>
        <td><%= tira.aprobado %></td>
        <td><%= link_to 'Show', tira %></td>
        <td><%= link_to 'Edit', edit_tira_path(tira) %></td>
        <td><%= link_to 'Destroy', tira, method: :delete, data: { confirm: 'Eliminar?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tira', new_tira_path %>
<%= link_to 'Logout', destroy_user_session_path, :method => 'delete' %>
